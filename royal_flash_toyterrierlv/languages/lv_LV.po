msgstr "Blenheima ( balts ar riekstu brūnu)"

#: page-form.php:231
#@ royal_flash
msgid "Tricolor"
msgstr "Trīskrāsu"

#: page-form.php:236
#@ royal_flash
msgid "What age Cavalier are you looking for?"
msgstr "Kāda vecuma Cavalierīti tu vēlies?"

#: page-form.php:241
#@ royal_flash
msgid "Puppy 9 - 16 weeks"
msgstr "Kucēnu 9 - 16 nedēļas"

#: page-form.php:243
#@ royal_flash
msgid "Puppy 4 months - 1 year"
msgstr "Kucēnu 4 mēnešu - 1 gads"

#: page-form.php:245
#@ royal_flash
msgid "Young Adult (over 1 year)"
msgstr "Jaunulis (over 1 year)"

#: page-form.php:247
#@ royal_flash
msgid "Retired Adult (Over age 3 and retired from show/breeding program)"
msgstr "Pieaudzis virs 3 gadu"

#: page-form.php:252
